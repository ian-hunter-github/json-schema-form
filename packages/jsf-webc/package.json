{
  "name": "@ianhunterpersonal/jsf-webc",
  "version": "0.0.1-beta.3",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./dist/styles/*": "./dist/styles/*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "./dist/styles/*"
  ],
  "dependencies": {
    "@ianhunterpersonal/jsf-vanilla": "^0.0.1-beta.3"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean && node ../jsf-react/scripts/copy-css.cjs"
  }
}
